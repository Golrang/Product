{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": ["./tsconfig.json"]
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": "error", "indent": "error", "no-multiple-empty-lines": "error",
        "no-multi-spaces": "error", "space-in-parens": "error",
        "no-var": "error", "prefer-const": "error",
        "no-use-before-define": "error", "no-console": "error",
        "no-debugger": "error"
    }
}
